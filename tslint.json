{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier", "tslint-plugin-prettier"],
  "rules": {
    "function-name": false,
    "import-name": false,
    "prettier": [
      true,
      {
        "trailingComma": "all",
        "printWidth": 100
      }
    ],
    "variable-name": false
  }
}
